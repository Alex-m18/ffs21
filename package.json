{
  "name": "13-diploma-backend",
  "version": "1.0.0",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "prestart": "npm i && npm run generate-key && npm run db-init",
    "start": "forever server.js",
    "watch": "forever -w server.js",
    "generate-key": "npx ./app/generateKey.js",
    "db-init": "npm run db-reset && npm run db-migrate && npm run db-seed",
    "db-reset": "db-migrate reset",
    "db-migrate": "db-migrate up",
    "db-seed": "npx ./database/seeders/databaseSeeder.js"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "db-migrate": "^0.11.11",
    "db-migrate-sqlite3": "^0.4.0",
    "dotenv": "^8.2.0",
    "forever": "^3.0.2",
    "fs": "^0.0.1-security",
    "koa": "^2.13.0",
    "koa-body": "^4.2.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^9.4.0",
    "koa2-cors": "^2.0.6",
    "koa2-validate": "^1.0.1",
    "moment": "^2.29.1",
    "paseto": "^1.0.7",
    "sqlite3": "^5.0.0",
    "uuid": "^8.3.1"
  }
}
